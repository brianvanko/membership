 <table>
	<tr>
			<h1><%= @user.name %></h1>
			<div class="address"?>
		    <%= @user.address %></br>
		    <%= @user.city %>, <%= @user.state %> <%= @user.zipcode %></br>
		    <%= @user.phone %></br></br>
		  </div>
		  <p>
			<%= link_to "Edit Info", edit_user_path %>
		  </p>
		
	</tr>
 </table>

<h3>Plans</h3>
<% @user.plans.each do |plan| %>
	<p>
		<b><%= plan.name %></b>($<%= plan.price %>)
	</p>
<% end %>

